{
  "name": "tce-i18n",
  "displayName": "TCE i18n",
  "description": "development tool for tce i18n project",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.34.0"
  },
  "icon": "images/icon.png",
  "publisher": "dickenslian",
  "repository": {
    "type": "git",
    "url": "https://github.com/dickenslian/vscode-tce-i18n.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.generateCode",
    "onCommand:extension.createTable"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateCode",
        "title": "Generate Code"
      },
      {
        "command": "extension.createTable",
        "title": "Create Table"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.generateCode",
          "group": "1_modification"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.createTable",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "clipboardy": "^2.0.0"
  }
}
